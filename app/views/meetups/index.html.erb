<h1>Plan a run</h1>
<p> Give your meetup spot a description </p>

<head>
  <script src="https://polyfill.io/v3/polyfill.min.js?features=default"></script>
</head>

<body>
    <%= render 'form', meetup: @meetup %>
    <input
      id="pac-input"
      class="controls"
      type="text"
      placeholder="Search Box"
    />
    <div style='width: 800px;'>
      <div id="map" style='width: 800px; height: 400px;'>
        <%= javascript_pack_tag 'mapLogic' %>
      </div>
    </div>
  <%= javascript_include_tag "//maps.googleapis.com/maps/api/js?key=#{@api_key}&callback=window.initAutocomplete&libraries=places&v=weekly" %>
</body>
